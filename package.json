{
  "name": "seattle-creative-directory",
  "description": "Phonebook for Seattle design community",
  "version": "1.2.0",
  "author": "Jonny McConnell <jonmccon@gmail.com>",
  "repository": "https://github.com/jonmccon/seattle-creative-directory",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop --H 0.0.0.0",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "gatsby clean",
    "ndev": "netlify dev",
    "functions": "netlify-lambda build ./functions",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "postinstall": "netlify-lambda install"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "@notionhq/client": "^2.2.2",
    "dotenv-webpack": "^8.0.1",
    "encoding": "^0.1.13",
    "gatsby": "^5.2.0",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-google-analytics": "^5.2.0",
    "gatsby-plugin-lodash": "^6.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-nprogress": "^5.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-plugin-twitter": "^5.2.0",
    "gatsby-remark-autolink-headers": "^6.2.0",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^6.2.0",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-remark-responsive-iframe": "^6.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "netlify-lambda": "^2.0.15",
    "netlify-plugin-checklinks": "^4.1.1",
    "node-fetch": "^3.3.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-h5-audio-player": "^3.8.6",
    "react-helmet": "^6.1.0",
    "react-is": "^18.2.0",
    "react-share": "^4.4.1",
    "react-twitter-widgets": "^1.11.0",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.6",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cli-glob": "^0.1.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "gh-pages": "^4.0.0",
    "immutable": "^4.1.0",
    "prettier": "^2.8.0",
    "redux": "^4.2.0",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.2",
    "slate": "^0.86.0",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.3",
    "write-good": "^1.0.8"
  },
  "keywords": [
    "seattle",
    "design",
    "contact",
    "community",
    "phonebook"
  ]
}
