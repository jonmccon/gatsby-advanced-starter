{
  "name": "seattle-creative-directory",
  "description": "Phonebook for Seattle design community",
  "version": "1.2.0",
  "author": "Jonny McConnell <jonmccon@gmail.com>",
  "repository": "https://github.com/jonmccon/seattle-creative-directory",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop --H 0.0.0.0",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "gatsby clean",
    "ndev": "netlify dev",
    "functions": "netlify-lambda build ./functions",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "postinstall": "netlify-lambda install"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "@notionhq/client": "^0.4.3",
    "dotenv-webpack": "^7.0.3",
    "encoding": "^0.1.13",
    "gatsby": "^3.14.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-feed": "^3.14.0",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-lodash": "^4.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-nprogress": "^3.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-plugin-twitter": "^3.14.0",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-responsive-iframe": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-simplecast": "^2.2.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "netlify-lambda": "^2.0.3",
    "netlify-plugin-checklinks": "^4.1.1",
    "node-fetch": "^3.0.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.9.0",
    "react-h5-audio-player": "^3.5.0",
    "react-helmet": "^6.1.0",
    "react-share": "^4.1.0",
    "react-ssg-netlify-forms": "^1.0.3",
    "react-twitter-widgets": "^1.7.1",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.0.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cli-glob": "^0.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^3.2.3",
    "immutable": "^4.0.0",
    "prettier": "^2.0.2",
    "redux": "^4.0.5",
    "remark-cli": "^10.0.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "slate": "^0.57.1",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "seattle",
    "design",
    "contact",
    "community",
    "phonebook"
  ]
}
